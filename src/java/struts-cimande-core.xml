	<package name="tree" extends="default" namespace="/backend">
		<action name="yuitree"
			class="org.blueoxygen.cimande.sitemanager.YUIExtNavTree">
			<result name="success" type="velocity">
				/view/backend/yuiexttree.vm
			</result>
			<result name="error">/errors/notfound.vm</result>
		</action>
		<action name="yuimenu" class="org.blueoxygen.cimande.sitemanager.MenuGenerator">
			<result name="success" type="velocity">
				/view/backend/yuimenu.vm
			</result>
		</action>
	</package>
	<!--Security - Login-->
	<package name="security" extends="default"
		namespace="/backend/user">
		<action name="index"
			class="org.blueoxygen.cimande.security.login.LoginForm">
			<result name="login" type="velocity">
				/view/frontpage/login/login.vm
			</result>
			<result name="site" type="velocity">
				/view/frontpage/login/site.vm
			</result>
			<result name="continue" type="redirect">
				../yuitree.action
			</result>
			<result name="menu" type="redirect">
				../yuimenu.action
			</result>
			<result name="redirect" type="redirect">
				../../${redirectUri}
			</result>
<!--			<result name="redirect" type="redirect">-->
<!--				../yuitree.action-->
<!--			</result>-->
			<result name="error">/errors/notfound.vm</result>
		</action>
		<action name="login"
			class="org.blueoxygen.cimande.security.login.Login">
			<result name="success" type="redirect">
				index.action?redirectUri=${redirectUri}
			</result>
			<result name="input" type="velocity">
				/view/frontpage/login/login.vm
			</result>
			<result name="error">/errors/notfound.vm</result>
		</action>
		<action name="site"
			class="org.blueoxygen.cimande.security.login.SiteSelection">
			<result name="success" type="redirect">
				../yuitree.action
			</result>
			<result name="redirect" type="redirect">
				../../${redirectUri}
			</result>
			<result name="input" type="redirect">
				/view/frontpage/login/site.vm
			</result>
		</action>
		<action name="logout"
			class="org.blueoxygen.cimande.security.login.Logout">
			<result name="success" type="redirect">index.action</result>
		</action>
		<action name="signup"
			class="org.blueoxygen.cimande.security.login.Signup">
			<result name="success" type="redirect">/</result>
			<result name="input">signup.jsp</result>
			<result name="error">/errors/notfound.vm</result>
		</action>
	</package>
