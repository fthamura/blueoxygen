			<result name="success" type="velocity">/view/frontpage/register/register.vm</result>
		</action>
		<action name="registeruser" class="org.blueoxygen.cimande.registration.RegisterUser">
			<result name="input" type="velocity">/view/frontpage/register/register.vm</result>
