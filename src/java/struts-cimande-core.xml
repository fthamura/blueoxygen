			</result>
		</action>
		<action name="finish"
			class="org.blueoxygen.cimande.security.rsp.actions.Master">
			<result name="success" type="velocity">
				/view/module/rsp/addSuccess.vm
			</result>
			<result name="error" type="velocity">
				/errors/errors.vm
			</result>
		</action>
