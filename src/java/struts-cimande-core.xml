			<result name="success" type="velocity">/view/frontpage/register/success.vm</result>
		</action>
		<action name="reqactmail" class="org.blueoxygen.cimande.registration.RegistrationForm">
			<result name="success" type="velocity">/view/frontpage/register/activation.vm</result>
		</action>
		<action name="sendactmail" class="org.blueoxygen.cimande.registration.ResendMail">
