<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!--  WINDOW  -->
	<package name="window" extends="default" namespace="/module/window">
		<action name="create" class="org.blueoxygen.cimande.gx.window.WindowForm">
			<result name="success" type="velocity">/WEB-INF/view/module/window/form.vm</result>
		</action>
		<action name="add" class="org.blueoxygen.cimande.gx.window.AddWindow">
			<result name="success" type="redirect">view.action?id=${Id}</result>
			<result name="input" type="velocity">/WEB-INF/view/module/window/form.vm</result>
		</action>
		<action name="view" class="org.blueoxygen.cimande.gx.window.WindowForm">
			<result name="success" type="velocity">/WEB-INF/view/module/window/form.vm</result>
		</action>
		<action name="filter" class="org.blueoxygen.cimande.gx.window.WindowForm">
			<result name="success" type="velocity">/WEB-INF/view/module/window/search.vm</result>
		</action>
		<action name="search" class="org.blueoxygen.cimande.gx.window.SearchWindow">
			<result name="success" type="velocity">/WEB-INF/view/module/window/result.vm</result>
		</action>
		<action name="edit" class="org.blueoxygen.cimande.gx.window.WindowForm">
			<result name="success" type="velocity">/WEB-INF/view/module/window/form.vm</result>
		</action>
		<action name="confirm" class="org.blueoxygen.cimande.gx.window.WindowForm">
			<result name="success" type="velocity">/WEB-INF/view/module/window/delete.vm</result>
		</action>
		<action name="delete" class="org.blueoxygen.cimande.gx.window.DeleteWindow">
			<result name="success" type="velocity">/WEB-INF/view/module/window/report.vm</result>
		</action>
		<action name="list" class="org.blueoxygen.cimande.gx.window.ListWindow">
			<result name="success" type="velocity">/WEB-INF/view/module/window/list.vm</result>
		</action>
	</package>

	<!--  GXFORM  -->
	<package name="generate" extends="default" namespace="/module/generate">
		<action name="create" class="org.blueoxygen.cimande.gx.tab.GxformForm">
			<result name="success" type="velocity">/WEB-INF/view/module/generate/form.vm</result>
		</action>
		<action name="add" class="org.blueoxygen.cimande.gx.tab.AddGxform">
			<result name="success" type="redirect">view.action?id=${id}</result>
			<result name="input" type="velocity">/WEB-INF/view/module/generate/form.vm</result>
		</action>
		<action name="view" class="org.blueoxygen.cimande.gx.tab.GxformForm">
			<result name="success" type="velocity">/WEB-INF/view/module/generate/view.vm</result>
		</action>
		<action name="filter" class="org.blueoxygen.cimande.gx.tab.GxformForm">
			<result name="success" type="velocity">/WEB-INF/view/module/generate/search.vm</result>
		</action>
		<action name="search" class="org.blueoxygen.cimande.gx.tab.SearchGxform">
			<result name="success" type="velocity">/WEB-INF/view/module/generate/result.vm</result>
		</action>
		<action name="edit" class="org.blueoxygen.cimande.gx.tab.GxformForm">
			<result name="success" type="velocity">/WEB-INF/view/module/generate/form.vm</result>
		</action>
		<action name="confirm" class="org.blueoxygen.cimande.gx.tab.GxformForm">
			<result name="success" type="velocity">/WEB-INF/view/module/generate/delete.vm</result>
		</action>
		<action name="delete" class="org.blueoxygen.cimande.gx.tab.DeleteGxform">
			<result name="success" type="velocity">/WEB-INF/view/module/generate/report.vm</result>
		</action>
		<action name="gen" class="org.blueoxygen.cimande.gx.tab.GxformForm">
			<result name="success" type="velocity">/WEB-INF/view/module/generate/view_gx.vm</result>
		</action>
	</package>

<!--  GXGREENATOR  -->
	<package name="gxgreenator" extends="default" namespace="/module/gxgreenator">
		<action name="add" class="org.blueoxygen.cimande.gx.field.AddGxgreenator"  >
			<result name="success" type="redirect">../generate/view.action?id=${gxformId}</result>
			<result name="input" type="redirect">../generate/view.action?id=${gxformId}</result>
		</action>
		<action name="delete" class="org.blueoxygen.cimande.gx.field.DeleteGxgreenator">
			<result name="success" type="redirect">../generate/view.action?id=${gxformId}</result>
		</action>
		<action name="edit" class="org.blueoxygen.cimande.gx.field.GxgreenatorForm">
			<result name="success" type="velocity">/WEB-INF/view/module/generate/view.vm</result>
		</action>
		<action name="gen" class="org.blueoxygen.cimande.gx.field.AddGxgreenator">
			<result name="success" type="velocity">/WEB-INF/view/module/gxgreenator/view_gx.vm</result>
		</action>
	</package>
	<package name="test" extends="default" namespace="/hehe">
		<action name="test" class="org.blueoxygen.cimande.gx.SubmitAction"  >
			<result name="success" type="velocity">/WEB-INF/view/hehe.vm</result>
		</action>
	</package>
</struts>