<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!--  WINDOW  -->
	<package name="window" extends="default" namespace="/module/window">
		<action name="create" class="org.blueoxygen.cimande.gx.window.WindowForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/window/form.vm</result>
		</action>
		<action name="add" class="org.blueoxygen.cimande.gx.window.AddWindow">
			<result name="success" type="redirect">view.action?id=${Id}</result>
			<result name="input" type="velocity">/WEB-INF/view/module/gx/window/form.vm</result>
		</action>
		<action name="view" class="org.blueoxygen.cimande.gx.window.WindowForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/window/form.vm</result>
		</action>
		<action name="filter" class="org.blueoxygen.cimande.gx.window.WindowForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/window/search.vm</result>
		</action>
		<action name="search" class="org.blueoxygen.cimande.gx.window.SearchWindow">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/window/result.vm</result>
		</action>
		<action name="edit" class="org.blueoxygen.cimande.gx.window.WindowForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/window/form.vm</result>
		</action>
		<action name="confirm" class="org.blueoxygen.cimande.gx.window.WindowForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/window/delete.vm</result>
		</action>
		<action name="delete" class="org.blueoxygen.cimande.gx.window.DeleteWindow">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/window/report.vm</result>
		</action>
		<action name="list" class="org.blueoxygen.cimande.gx.window.ListWindow">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/window/list.vm</result>
		</action>
	</package>

	<!--  GXFORM  -->
	<package name="tab" extends="default" namespace="/module/tab">
		<action name="create" class="org.blueoxygen.cimande.gx.tab.TabForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/tab/form.vm</result>
		</action>
		<action name="add" class="org.blueoxygen.cimande.gx.tab.SaveTab">
			<result name="success" type="redirect">view.action?id=${id}</result>
			<result name="input" type="velocity">/WEB-INF/view/module/gx/tab/form.vm</result>
		</action>
		<action name="view" class="org.blueoxygen.cimande.gx.tab.TabForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/tab/view.vm</result>
		</action>
		<action name="filter" class="org.blueoxygen.cimande.gx.tab.TabForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/tab/search.vm</result>
		</action>
		<action name="search" class="org.blueoxygen.cimande.gx.tab.SearchTab">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/tab/result.vm</result>
		</action>
		<action name="edit" class="org.blueoxygen.cimande.gx.tab.TabForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/tab/form.vm</result>
		</action>
		<action name="confirm" class="org.blueoxygen.cimande.gx.tab.TabForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/tab/delete.vm</result>
		</action>
		<action name="delete" class="org.blueoxygen.cimande.gx.tab.DeleteTab">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/tab/report.vm</result>
		</action>
		<action name="gen" class="org.blueoxygen.cimande.gx.tab.TabForm">
			<result name="success" type="velocity">/WEB-INF/view/module/gx/tab/view_gx.vm</result>
		</action>
	</package>

<!--  GXGREENATOR  -->
	<package name="field" extends="default" namespace="/module/field">
		<action name="add" class="org.blueoxygen.cimande.gx.field.SaveField"  >
			<result name="success" type="redirect">../tab/view.action?id=${gxformId}</result>
			<result name="input" type="redirect">../tab/view.action?id=${gxformId}</result>
		</action>
		<action name="delete" class="org.blueoxygen.cimande.gx.field.DeleteField">
			<result name="success" type="redirect">../tab/view.action?id=${gxformId}</result>
		</action>
		<action name="edit" class="org.blueoxygen.cimande.gx.field.FieldForm">
			<result name="success" type="velocity">/WEB-INF/view/module/tab/view.vm</result>
		</action>
		<action name="gen" class="org.blueoxygen.cimande.gx.field.SaveField">
			<result name="success" type="velocity">/WEB-INF/view/module/field/view_gx.vm</result>
		</action>
	</package>
	
</struts>