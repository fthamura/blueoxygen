<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="actionType" extends="default" namespace="/module/actiontype">
		<interceptors>
			<interceptor name="cimande-acl" class="org.blueoxygen.debus.interceptors.ACLInterceptor"/>
		</interceptors>
		<action name="create" class="org.blueoxygen.debus.actions.ActionTypeAction" method="execute">
			<interceptor-ref name="cimande-acl"/>
			<result name="success" type="velocity">/view/debus/actiontype/form.vm</result>
		</action>
		<action name="save" class="org.blueoxygen.debus.actions.ActionTypeAction" method="save">
			<interceptor-ref name="cimande-acl"/>
			<result name="success" type="velocity">/view/debus/actiontype/success.vm</result>
			<result name="input" type="velocity">/view/debus/actiontype/form.vm</result>
		</action>
	</package>
	<package name="roleDesriptor" extends="default" namespace="/module/roledescriptor">
		<action name="create" class="org.blueoxygen.debus.actions.roledescriptor.RoleDescriptorACLForm">
			<result name="success" type="velocity">/view/debus/roledescriptor/form.vm</result>
		</action>
		<action name="save" class="org.blueoxygen.debus.actions.roledescriptor.SaveRoleDescriptorACL">
			<result name="success" type="velocity">/view/debus/roledescriptor/success.vm</result>
			<result name="input" type="velocity">/view/debus/roledescriptor/form.vm</result>
		</action>
		<action name="filter" class="org.blueoxygen.debus.actions.roledescriptor.RoleDescriptorACLForm">
			<result name="success" type="velocity">/view/debus/roledescriptor/search.vm</result>
		</action>
		<action name="search" class="org.blueoxygen.debus.actions.roledescriptor.SearchRoleDescriptorACL">
			<result name="success" type="velocity">/view/debus/roledescriptor/result.vm</result>
		</action>
		<action name="listrole" class="org.blueoxygen.cimande.role.actions.ListRole">
			<result name="success" type="velocity">/view/module/role/listrole.vm</result>
		</action>
		<action name="listdescriptor" class="org.blueoxygen.cimande.moduledescriptor.actions.ListModuleDescriptor">
			<result name="success" type="velocity">/view/module/descriptor/listdescriptor.vm</result>
		</action>
		<action name="listaccess" class="org.blueoxygen.cimande.gx.droplist.DroplistForm">
			<result name="success" type="velocity">/view/module/gx/droplist/listvalue.vm</result>
		</action>
		<action name="delete" class="org.blueoxygen.debus.actions.roledescriptor.RoleDescriptorACLForm">
			<result name="success" type="velocity">/view/debus/roledescriptor/delete.vm</result>
		</action>
		<action name="confirm" class="org.blueoxygen.debus.actions.roledescriptor.DeleteRoleDescriptorACL">
			<result name="success" type="velocity">/view/debus/roledescriptor/delete.vm</result>
		</action>
	</package>

</struts>