<?xml version="1.0"?>
<?cocoon-process type="xsp"?>
<?cocoon-process type="xslt"?>
<?xml-stylesheet href="view_approval_history.html.xsl" type="text/xsl"?>
<xsp:page xmlns:xsp="http://www.apache.org/1999/XSP/Core" xmlns:session="http://www.apache.org/1999/XSP/Session" create-session="true">
  <page>
    <p>
    <xsp:logic>
    	String sGA_USER;
    	sGA_USER=(String)session.getAttribute("GA_USER");
        if (sGA_USER!=null) {
        	<xsp:content>
        		GA_USER is: <session:get-attribute name="GA_USER"/>
        		Your session was created: <session:get-creation-time as="string"/>
      			<pagex name="hehe"/>
      		</xsp:content>
      	} else {
      		<xsp:content>
        		You are not logged in
      		</xsp:content>
      	}
    </xsp:logic>
    </p>
  </page>
</xsp:page>
