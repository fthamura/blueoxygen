<?xml version="1.0"?>
<?xml-stylesheet href="/xsl/descriptor.xsp.xsl" type="text/xsl"?>
<?cocoon-process type="xslt"?>
<contentmanager name="sni_member" method="post" action="../utility/crvd-engine.jsp">

	
	<descriptor title="Member Code"  name="membership_code" maxlength="35" type="text" size="35" required="yes" search="yes" edit="yes">
		<builder datatype="varchar" datasize="35"/>
	</descriptor>
	
	<descriptor title="MCode 1"  comment="2 digit kode telp" name="membership_code_interlokal" maxlength="2" type="text" size="2" required="yes" search="yes" edit="yes">
		<builder datatype="varchar" datasize="2"/>
	</descriptor>

	<descriptor title="MCode 2" comment="Kode Pos 4 digit" name="membership_code_kode_pos" maxlength="4" type="text" size="4" required="yes" search="yes" edit="yes">
		<builder datatype="varchar" datasize="4"/>
	</descriptor>

	<descriptor title="MCode 3" name="membership_code_digit" maxlength="9" type="text" size="9" required="yes" search="yes" edit="yes">
		<builder datatype="varchar" datasize="9"/>
	</descriptor>

	<descriptor title="First Name" name="first_name" maxlength="35" type="text" size="25" required="yes" search="yes" edit="yes">
		<builder datatype="varchar" datasize="35"/>
	</descriptor>

	<descriptor title="Last Name" name="last_name" maxlength="15" type="text" size="15" search="yes" edit="yes">
		<builder datatype="varchar" datasize="35"/>
	</descriptor>
	
	<descriptor title="Upline" name="parent_id" maxlength="28" type="text" size="15" required="yes" search="yes" edit="yes" >
		<builder datatype="varchar" datasize="28" />
	</descriptor>

	<descriptor title="Referer" name="referer_id" maxlength="28" type="text" size="15" required="yes" search="yes" edit="yes" >
		<builder datatype="varchar" datasize="28" />
	</descriptor>
	
	<descriptor title="Password" name="password" maxlength="15" type="password" size="15" required="yes" edit="yes">
		<builder datatype="varchar" datasize="15"/>
	</descriptor>


	<descriptor title="Email" name="email" maxlength="125" type="text" size="25" search="yes" edit="yes">
		<builder datatype="varchar" datasize="125"/>
	</descriptor>

	<descriptor title="Address" name="address1" maxlength="55" type="text" size="35" edit="yes">
		<builder datatype="varchar" datasize="55"/>
	</descriptor>

	<descriptor title="" name="address2" maxlength="55" type="text" size="35" edit="yes">
		<builder datatype="varchar" datasize="35"/>
	</descriptor>

	<descriptor title="City" name="city" maxlength="15" type="text" size="15" edit="yes">
		<builder datatype="varchar" datasize="15"/>
	</descriptor>

</contentmanager>
