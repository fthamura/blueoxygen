<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
  <title>Workspace Management - Search a Module Function</title>
  <meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
  <meta name="GENERATOR" content="Quanta Plus">
  <link type="text/css" rel="stylesheet" href="../../style.css">
  <script type="text/javascript">
  _editor_url = "../../htmlarea/";
  _editor_lang = "en";
</script>

<script type="text/javascript" src="../../htmlarea/htmlarea.js"></script>
</head>
<body onload="HTMLArea.replaceAll();">
#if (!$actionErrors.isEmpty())
<div class="errorMessage">Errors</div>
<ul class="errorMessage">
#foreach( $error in $actionErrors ) <li>$error</li>
#end
</ul>
#end
<body>
<form name="filterModuleFunction" method="post" action="searchModuleFunction.action">
<table bgcolor="gray" align="center" border="0" cellpadding="5" cellspacing="1" width="98%">
  <tbody>
  <tr bgcolor="#d0dcff">
    <td colspan="3"><b>Search Module Function<b></td>
  </tr>
  <tr bgcolor="#ffffff">
    <td width="100">Name:<font color="RED">*</font></td>
    <td><input type="text" name="name" maxlength="30" value="$!name" size="30"></td>
    <td align="right" width="3%" valign="center"><img alt="Name of The Descriptor" src="../../images/help.gif" border="0"></td>
  </tr>
  <tr bgcolor="#ffffff"> 
    <td width="100">Description:</td>
    <td><input type="text" name="description" maxlength="30" value="$!description" size="30"></td>
  	<td align="right" width="3%" valign="center"><img alt="Name of The Descriptor" src="../../images/help.gif" border="0"></td>
  </tr>
  
  <tr bgcolor="#ffffff">
  	<td>Table References:</td>
    <td><input type="text" name="tableReferences" value="$!tableReferences" size="30"></td>
  	<td align="right"width="3%" valign="center"><img alt="Name of The Descriptor" src="../../images/help.gif" border="0"></td>
  </tr>
    <tr bgcolor="#d0dcff">
  <td colspan="3"><input type="submit" value="Search">&nbsp;&nbsp;<input type="reset" value="Reset"></td>
  </tr>
 
</table>
	
</body>

</html>
