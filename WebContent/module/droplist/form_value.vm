	<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
	<!--<%@ taglib uri = "webwork" prefix = "ww" %>-->
	<html>
		<head>
			<title>Form Droplist - Add Value</title>
			<link rel="stylesheet" type="text/css" href="../../style.css">
		</head>
		<body>
			#if (!$actionErrors.isEmpty())
				<div class="errorMessage">Errors</div>
				<ul class="errorMessage">
				#foreach( $error in $actionErrors )
					<li>$error</li>
				#end
				</ul>
			#end
			
			<form method="get" action="add.action">
				<table border="0" cellpadding="5" cellspacing="1" width="98%" bgcolor="#000000">
					<tr bgcolor="#d0dcff">
						
      <td colspan="2">Add / Edit Value</td>
					</tr>
					<tr bgcolor="#ffffff">
						<td>Value : <font color="red">*</font></td>
						<td>
							<input name="droplist.value" value="$!droplist.value"/>
						</td>
						</td>
					</tr>
					<tr bgcolor="#ffffff">
						<td>Description : <font color="red">*</font></td>
						<td>
							<input name="droplist.description" value="$!droplist.description"/>
						</td>
					</tr>
					<tr bgcolor="#ffffff">
					</tr>
					<tr bgcolor="#e4e4e4">
						
      <td colspan="2" align="right"> 
        <input name="id" type="hidden" value="$!droplist.id"/>
        <input name="submit" type="submit" value="Submit Value"/> </td>
					</tr>
					<!--tr bgcolor="#ffffff">
					    <td colspan="2">
					      	<a href="../form.action">Home</a> <a href="../form.action">Back</a></td>
					</tr-->
				</table>
			</form>
		</body>
	</html>