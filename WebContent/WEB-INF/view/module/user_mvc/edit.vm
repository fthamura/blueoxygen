<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN">
<html>
<head>
<title>Workspace Management - Edit Company</title>
  <link type="text/css" rel="stylesheet" href="../../style.css">
  <script type="text/javascript">
  _editor_url = "../../htmlarea/";
  _editor_lang = "en";
</script>

<script type="text/javascript" src="../../htmlarea/htmlarea.js"></script>
</head>
<body onload="HTMLArea.replaceAll();">
#if (!$actionErrors.isEmpty())
<div class="errorMessage">Errors</div>
<ul class="errorMessage">
#foreach( $error in $actionErrors ) <li>$error</li>
#end
</ul>
#end

<form method="post" action="update.action">
<table bgcolor="gray" border="0" cellpadding="5" cellspacing="1" width="98%">
  <tr bgcolor="#d0dcff">
        <td colspan="3"><b>Edit User Role </b></td>
  </tr>
  <tr bgcolor="#ffffff">
    <td style="font-weight: bold;" align="right" width="25%"><font color="red">* </font>Login:</td>
    <td><input type="text" name="name" value="$!username" maxlength="128">
  </tr>
  <tr bgcolor="#ffffff">
        <td style="font-weight: bold;" align="right" width="25%"><font color="red">* </font>Password: </td>
    <td><input type="text" name="address" value="$!password" >
  </tr> 
  <tr bgcolor="#ffffff">
  	<td style="font-weight: bold;" align="right" width="25%"><font color="red">* </font>First Name:</td>
    <td><input type="text" name="state" value="$!first_name" maxlength="128">
  </tr>
  <tr bgcolor="#ffffff">
  	<td style="font-weight: bold;" align="right" width="25%"><font color="red">*</font>Company: </td>
    <td><input type="textfield" id="perusahaanName" value="None" size="15" disabled="true" onfocus="this.blur()"/> 
      <input type="hidden" id="companyId" name="companyId" value="0"/> <input type="button" value="View Company" onclick="javascript:window.open('../user_mvc/viewCompany.action','cats','scrollbars=yes,status=no,width=300,height=300')"/> </tr>
  <tr bgcolor="#ffffff">
  	<td style="font-weight: bold;" align="right" width="25%"><font color="red">* </font>Position: </td>
    <td> <input type="textfield" id="jabatanName" value="None" size="15" disabled="true" onfocus="this.blur()"/> <input type="hidden" id="viewjobId" name="viewjobId" value=""/>
  <input type="button" value="View Jabatan" onclick="javascript:window.open('../user_mvc/viewJob.action','cats','scrollbars=yes,status=no,width=300,height=300')"/>     </tr>
  <tr bgcolor="#ffffff">
  	<td style="font-weight: bold;" align="right" width="25%">Email: </td>
    <td><input type="text" name="website" value="$!email" maxlength="128">
  </tr>
  <tr bgcolor="#ffffff">
    <td style="font-weight: bold;" align="right">Address: </td>
    <td>  <input type="text" name="website" value="$!address1" maxlength="128">
  </tr>
  <tr bgcolor="#ffffff">
    <td style="font-weight: bold;" align="right">&nbsp;</td>
    <td>  <input type="text" name="website" value="$!address2" maxlength="128">
  </tr>
  <tr bgcolor="#ffffff">
    <td style="font-weight: bold;" align="right">City:</td>
    <td> <input type="text" name="website" value="$!city" maxlength="128"> 
  </tr>
  <tr bgcolor="#ffffff">
  <td style="font-weight: bold;" align="right" width="25%">Description: </td>
    <td> <textarea cols="35" name="description" valuer="$!description" rows="5"></textarea> </tr>
  <tr bgcolor="#d0dcff">
  	<td colspan="2">
  	<input type="hidden" name="id" value="$id">
  	<input type="submit" value="Update"><input type="reset" value="Reset">
</td>
  	</td>
  </tr>
</table>



</form>

<p>&nbsp;</p>
</body>
</html>
