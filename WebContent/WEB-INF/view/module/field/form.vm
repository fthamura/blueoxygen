<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!--<%@ taglib uri = "webwork" prefix = "ww" %>-->
<html>
	<head>
		<title>Form - Gxgreenator</title>
		<link rel="stylesheet" type="text/css" href="../../style.css">
	</head>
	<body>
		#if (!$actionErrors.isEmpty())
			<div class="errorMessage">Errors</div>
			<ul class="errorMessage">
			#foreach( $error in $actionErrors )
				<li>$error</li>
			#end
			</ul>
		#end
		
		<form method="get" action="add.action">
			<table border="0" cellpadding="5" cellspacing="1" width="98%" bgcolor="#000000">
				<tr bgcolor="#d0dcff">
					<td colspan="2">Add / Edit Gxgreenator</td>
				</tr>
				<tr bgcolor="#ffffff">
					<td>Name : <font color="red">*</font></td>
					<td>
						<input name="gxgreenator.name" value="$!gxgreenator.name"/>
					</td>
				</tr>
				
				<tr bgcolor="#ffffff">
					<td>Gxform id : <font color="red">*</font></td>
					<td>
						<input name="gxgreenator.gxform_id" value="$!gxgreenator.gxform_id"/>
					</td>
				</tr>
				<tr bgcolor="#e4e4e4">
					<td colspan="2">
						<input type="submit" value="Submit"/>
						<input name="id" type="hidden" value="$!gxgreenator.id"/>
					</td>
				</tr>
				<tr bgcolor="#ffffff">
					<td colspan="2"><a href="../form.action">Home</a></td>
				</tr>
			</table>
		</form>
	</body>
</html>