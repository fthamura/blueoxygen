<html>
<head>
	<title>Workspace Management - Result of Search Descriptor </title>
  	<link type="text/css" rel="stylesheet" href="../../style.css">
</head>
<body>
<table bgcolor="gray" border="0" cellpadding="5" cellspacing="1" width="98%">
      <tr bgcolor="#d0dcff">
        <td colspan="5"><b>Search Descriptor Group</b></td>
      </tr>

#if ($desGroups.isEmpty()) 
	<tr bgcolor="yellow">
        	<td colspan="5" align="right">No such data found</td>
      	</tr>
#else 
	<tr bgcolor="yellow">
        	<td colspan="5" align="right">
        	Found ( <strong>$!resultRows</strong> ) #if ($resultRows > 1) descriptor groups #else descriptor group
        	</td>
      	</tr>
#end
<tr bgcolor="#e4e4e4" width="98%">
	
	<td><b>Group ID</b></td>
	<td><b>Folder</b></td>
	<td><b>Description</b></td>
	<td><b>Action</b></td>
</tr>

#foreach ($desGroup in $desGroups)
	<tr valign="top" bgcolor="white">
	
	<td><a href="edit.action?id=$desGroup.Id">$!desGroup.GroupId</a></td>
	<td><a href="edit.action?id=$desGroup.Id">$!desGroup.Folder</a></td>
	<td><a href="edit.action?id=$desGroup.Id">$!desGroup.Description</a></td>
	
	<td>
		<a href="edit.action?id=$desGroup.Id">Edit</a>&nbsp;
		<a href="confirmDelete.action?id=$desGroup.Id">Del</a>
	</td>
	</tr>
#end
	<tr bgcolor="#e4e4e4">
		<td align="right" colspan="5">
			#if ($currPage > 0) <a href="search.action?groupId=$groupId&folder=$folder&description=$description&activeFlag=$activeFlag&currPage=0">&lt;&lt;</a>&nbsp;#end
			#if ($currPage > 0) <a href="search.action?groupId=$groupId&folder=$folder&description=$description&activeFlag=$activeFlag&currPage=$prevPage">&lt;</a>&nbsp;#end
			[page $page]
			#if ($currPage < $maxPage) <a href="search.action?groupId=$groupId&folder=$folder&description=$description&activeFlag=$activeFlag&currPage=$nextPage">&gt;</a>&nbsp;#end
			#if ($currPage < $maxPage) <a href="search.action?groupId=$groupId&folder=$folder&description=$description&activeFlag=$activeFlag&currPage=$maxPage">&gt;&gt;</a>&nbsp;#end
		</td>
     </tr>
</table>

#end

</body>
</html>
