  #if(!$window.tabs.isEmpty())
  <table align="center" bgcolor="gray" border="0" cellpadding="5" cellspacing="1" width="98%">
    <tbody>
		<tr>
			<td colspan="3" bgcolor="#333366"><span class="style1">Tab</span></td>
		</tr>
	<tr id="tableHeader1" bgcolor="#CFE9EB"> 
    	<td>Name</td>
    	<td>Description</td>
    	<td>Action</td>
  	</tr>
  #foreach ($de in $window.tabs) 
  <tr valign="top" bgcolor="#ffffff"> 
    <td><a href="../tab/edit.action?window.id=$!window.id&tab.id=$!de.id">$!de.name</a></td>
    <td>$!de.description</td>
    <td>
    	<a href="edit.action?window.id=$!window.id&tab.id=$!de.id"><img src="../../images/silk/page_edit.png" border="0"></a> 
        <a href="delete.action?window.id=$!window.id&tab.id=$!de.id"><img src="../../images/silk/delete.png" border="0"></a>
    </td>
  </tr>
  #end 
  </tbody>
  </table>
  #end
  