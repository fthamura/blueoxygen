<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!--<%@ taglib uri = "webwork" prefix = "ww" %>-->
<html>
	<head>
		<title>Search - Table</title>
		<link rel="stylesheet" type="text/css" href="../../style.css">
	</head>
	<body>
		#if (!$actionErrors.isEmpty())
			<div class="errorMessage">Errors</div>
			<ul class="errorMessage">
			#foreach( $error in $actionErrors )
				<li>$error</li>
			#end
			</ul>
		#end
		
		<form method="get" action="search.action">
			<table border="0" cellpadding="5" cellspacing="1" width="98%" bgcolor="#000000">
				<tr bgcolor="#d0dcff">
					<td colspan="2"><b>Search Table</b></td>
				</tr>
				<tr bgcolor="#ffffff">
					<td>Name :</td>
					<td>
						<input name="table.name" value="$!table.name"/>
					</td>
				</tr>
				<tr bgcolor="#ffffff">
					<td>Description :</td>
					<td>
						<input name="table.description" value="$!table.description"/>
					</td>
				</tr>
				<tr bgcolor="#ffffff">
					<td>Table name :</td>
					<td>
						<input name="table.tableName" value="$!table.tableName"/>
					</td>
				</tr>
				<tr bgcolor="#e4e4e4">
					<td colspan="2">
						<input type="submit" value="Submit"/>
					</td>
				</tr>
			</table>
		</form>
		<br>
		<!--table bgcolor="gray" border="0" cellpadding="5" cellspacing="1" width="100%">
		  	<tr bgcolor="#ffffff">
				<td align="center"><a href="../../thin/form.action">Home</a></td>
			</tr>
		</table-->
	</body>
</html>