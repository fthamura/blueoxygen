<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<!--<%@ taglib uri = "webwork" prefix = "ww" %>-->
<html>
	<head>
		<title>Search - Module Column</title>
		<link rel="stylesheet" type="text/css" href="../../style.css">
	</head>
	<body>
		#if (!$actionErrors.isEmpty())
			<div class="errorMessage">Errors</div>
			<ul class="errorMessage">
			#foreach( $error in $actionErrors )
				<li>$error</li>
			#end
			</ul>
		#end
		
		<form method="get" action="search.action">
			<table border="0" cellpadding="5" cellspacing="1" width="98%" bgcolor="#000000">
				<tr bgcolor="#d0dcff">
					<td colspan="2">Search Column</td>
				</tr>
				<tr bgcolor="#ffffff">
					<td>Name :</td>
					<td>
						<input name="column.name" value="$!column.name"/>
					</td>
				</tr>
				<tr bgcolor="#ffffff">
					<td>Description :</td>
					<td>
						<input name="column.description" value="$!column.description"/>
					</td>
				</tr>
				<tr bgcolor="#ffffff">
					<td>Table id :</td>
					<td>
						<input name="column.table_id" value="$!column.action"/>
					</td>
				</tr>
				<tr bgcolor="#e4e4e4">
					<td colspan="2">
						<input type="submit" value="Submit"/>
					</td>
				</tr>
				<!--tr bgcolor="#ffffff">
					<td colspan="2"><a href="../../thin/form.action">Home</a></td>
				</tr-->
			</table>
		</form>
	</body>
</html>