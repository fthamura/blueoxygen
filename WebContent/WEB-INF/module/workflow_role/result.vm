<html>
<head>
	<title>Workspace Management - Result of Search Workflow Role</title>
  	<link type="text/css" rel="stylesheet" href="../../style.css">
</head>
<body>
<table bgcolor="gray" border="0" cellpadding="5" cellspacing="1" width="98%">
      <tr bgcolor="#d0dcff">
        <td colspan="5"><b>Search Workflow Role</b></td>
      </tr>

#if ($roles.isEmpty()) 
	<tr bgcolor="yellow">
        	<td colspan="5" align="right">No such data found</td>
      	</tr>
#else 
	<tr bgcolor="yellow">
        	<td colspan="5" align="right">
        	Found ( <strong>$!resultRows</strong> ) #if ($resultRows > 1) roles #else role 
        	</td>
      	</tr>
#end
<tr bgcolor="#e4e4e4" width="98%">
	
	<td><b>Name</b></td>
	<td><b>Description</b></td>
	<td width=9%><b>Action</b></td>
</tr>

#foreach ($moduleRole in $roles)
	<tr valign="top" bgcolor="white">
	
	<td><a href="edit.action?id=$moduleRole.Id">$!moduleRole.Name</a></td>
	<td><a href="edit.action?id=$moduleRole.Id">$!moduleRole.Description</a></td>
	
	<td>
		<a href="edit.action?id=$moduleRole.Id">Edit</a>&nbsp;
		<a href="confirmDelete.action?id=$moduleRole.Id">Del</a>
	</td>
	</tr>
#end
	<tr bgcolor="#e4e4e4">
		<td align="right" colspan="5">
			#if ($currPage > 0) <a href="search.action?name=$name&description=$description&parentId=$parentId&activeFlag=$activeFlag&currPage=0">&lt;&lt;</a>&nbsp;#end
			#if ($currPage > 0) <a href="search.action?name=$name&description=$description&parentId=$parentId&activeFlag=$activeFlag&currPage=$prevPage">&lt;</a>&nbsp;#end
			[page $page]
			#if ($currPage < $maxPage) <a href="search.action?name=$name&description=$description&parentId=$parentId&activeFlag=$activeFlag&currPage=$nextPage">&gt;</a>&nbsp;#end
			#if ($currPage < $maxPage) <a href="search.action?name=$name&description=$description&parentId=$parentId&activeFlag=$activeFlag&currPage=$maxPage">&gt;&gt;</a>&nbsp;#end
		</td>
     </tr>
</table>

#end

</body>
</html>
